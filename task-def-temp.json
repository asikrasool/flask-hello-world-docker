{
    "family": "flask-default",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "image": "343337789422.dkr.ecr.ap-south-1.amazonaws.com/flaskapp:latest",
            "taskRoleArn": "arn:aws:iam::343337789422:role/ecsTaskExecutionRole",
            "name": "flask-default",
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80
                }
            ]
        }
    ],
     "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512",
   
}
