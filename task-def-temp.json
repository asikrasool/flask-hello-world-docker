{
    "family": "flask-app",
    "executionRoleArn": "arn:aws:iam::343337789422:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "image": "343337789422.dkr.ecr.ap-south-1.amazonaws.com/flaskapp:v_%BUILD_NUMBER%",
            "name": "flask-app",
            "cpu": 10,
            "memory": 256,
            "essential": true,
            "logConfiguration": {
                "logDriver": "awslogs",
                "secretOptions": "None",
                "options": {
                  "awslogs-group": "/ecs/flask-app",
                  "awslogs-region": "ap-south-1",
                  "awslogs-stream-prefix": "ecs"
                }
            },
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "taskRoleArn": "arn:aws:iam::343337789422:role/ecsTaskExecutionRole",
    "cpu": "256",
    "memory": "512"
}